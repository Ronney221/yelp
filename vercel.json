{
    "version": 2,
    "builds": [
      { "src": "api/yelp.py", "use": "@vercel/python" },
      { "src": "api/analyze.py", "use": "@vercel/python" },
      { "src": "*.html", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/yelp", "dest": "/api/yelp.py" },
      { "src": "/api/analyze", "dest": "/api/analyze.py" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }